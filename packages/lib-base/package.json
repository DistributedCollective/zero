    "@typescript-eslint/parser": "4.18.0",
    "eslint": "7.22.0",
    "eslint-plugin-tsdoc": "0.2.11",
    "fast-check": "2.13.0",
    "mocha": "8.3.2",
    "npm-run-all": "4.1.5",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
